// .vscode/settings.json
{
    "deno.enable": true,
    "deno.import_map": "./path/to/import_map.json",
    "deno.unstable": false,
    "deno.lint": true,
    "deno.import_intellisense_origins": {
        "https://deno.land": true,
    },
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
}